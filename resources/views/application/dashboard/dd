        jquery code for card and card-content convert to collapsible and collapsible-body if url contain dashboard
        in material ui in php larvel 8


        $(document).ready(function() {
            // Check if the URL contains 'dashboard'
            if (window.location.href.indexOf("dashboard") !== -1) {
                // Convert the card and card-content to collapsible components
                $('#myCard').addClass('collapsible');
                $('#myCardContent').addClass('collapsible-body');

                // Initialize collapsible functionality (Materialize CSS)
                $('.collapsible').collapsible();
            }
        });
